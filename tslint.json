{
    "rules": {
        "eofline": false,
        "indent": [
            true,
            "tabs",
            2
        ],
        "space-within-parens": [
            true,
            0
        ],
        "max-line-length": {
            "options": [
                120
            ]
        },
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            },
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "space-before-function-paren": false
    }
}